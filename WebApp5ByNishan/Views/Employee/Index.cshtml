@model IEnumerable<WebApp5ByNishan.Models.Employees>

<table class="table">
    <thead>
        <tr>
            <th>EmpId</th>
            <th>EmpName</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.EmpId</td>
                <td>@employee.EmpName</td>
                <td>@employee.Salary</td>
            </tr>
        }
    </tbody>
</table>
